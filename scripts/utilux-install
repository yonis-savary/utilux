binName="$1"

if [ ! -f "$binName" ]
then
    echo "Cannot install, [$binName] is not a file"
    exit 1
fi

targetBinName="$UTILUX_BIN_PATH/$binName"

if [ -f "$targetBinName" ]
then
    echo "[$targetBinName] already exists !"
    exit 1
fi

echo "Copying $binName to $targetBinName"
ln -s "$binName" "$targetBinName"